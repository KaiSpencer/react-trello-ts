{
	"name": "react-trello-ts",
	"version": "2.1.1",
	"description": "Pluggable typesafe components to add a trello like kanban board to your application",
	"main": "./dist/index.mjs",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README"
	],
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"prepublish": "pnpm build",
		"storybook": "storybook dev -p 6006",
		"storybook:no-open": "pnpm storybook --no-open",
		"test": "start-server-and-test storybook:no-open http://localhost:6006 test-storybook",
		"test-storybook": "test-storybook",
		"build": "unbuild",
		"docs": "storybook build -o docs",
		"build-storybook": "storybook build -o storybook-build",
		"lint": "pnpm biome check .",
		"release": "pnpm build && pnpm changeset publish",
		"changeset": "changeset"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KaiSpencer/react-trello-ts"
	},
	"keywords": [
		"react",
		"trello",
		"board",
		"typescript"
	],
	"author": "Kai Spencer",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KaiSpencer/react-trello-ts/issues"
	},
	"homepage": "https://github.com/KaiSpencer/react-trello-ts",
	"dependencies": {
		"react-popopo": "^2.1.9",
		"react-trello-ts-smooth-dnd": "^0.0.3",
		"uuid": "^11.1.0",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.15.3",
		"@biomejs/biome": "1.4.1",
		"@changesets/cli": "^2.27.5",
		"@chromatic-com/storybook": "^1.5.0",
		"@microsoft/api-extractor": "^7.47.0",
		"@storybook/addon-essentials": "^8.6.4",
		"@storybook/cli": "^8.6.4",
		"@storybook/core-events": "^8.6.4",
		"@storybook/react": "^8.6.4",
		"@storybook/react-vite": "^8.6.4",
		"@storybook/test-runner": "^0.22.0",
		"@types/node": "^18.19.39",
		"@types/react": "^16.14.60",
		"@types/react-dom": "^16.9.24",
		"@types/styled-components": "^5.1.34",
		"@types/uuid": "^10",
		"i18next": "^17.3.1",
		"playwright": "^1.51.0",
		"publint": "^0.3.8",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-i18next": "^10.13.2",
		"start-server-and-test": "^2.0.4",
		"storybook": "^8.6.4",
		"styled-components": "^5.3.11",
		"typescript": "^5.8.2",
		"unbuild": "3.5.0"
	},
	"peerDependencies": {
		"react": ">=16.8.0 <=18",
		"react-dom": ">=16.8.0 <=18",
		"styled-components": ">= 4.0.3"
	},
	"packageManager": "pnpm@10.6.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@swc/core",
			"esbuild"
		]
	}
}
